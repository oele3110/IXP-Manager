<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Entities\RpkiValidation" repository-class="Repositories\RpkiValidation" table="rs_rpki_validation" tableName="rs_rpki_validation">

  	<id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="validity" type="string" nullable="true"/>
    <field name="info" type="string" nullable="true"/>
    <many-to-one field="RSPrefix" target-entity="Entities\RSPrefix" inversed-by="RpkiValidation">
      <join-columns>
        <join-column name="rs_prefix_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="RpkiRoa" target-entity="Entities\RpkiRoa" inversed-by="RpkiValidation">
      <join-columns>
        <join-column name="rs_prefix_id" referenced-column-name="rs_prefix_id"/>
      </join-columns>
    </many-to-one>
    <!--
    <many-to-one field="Customer" target-entity="Entities\Customer" inversed-by="RpkiValidation">
      <join-columns>
        <join-column name="custid" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    -->

  </entity>
</doctrine-mapping>
